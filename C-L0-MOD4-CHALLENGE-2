import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt(),count=0;
        while(a!=0){
          if((a%10)==b){
              count++;
          }
            a=a/10;
            
            

        }
         System.out.println(count);
    }
}
