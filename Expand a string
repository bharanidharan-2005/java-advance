import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String s = sc.next();
        for(int i = 0; i < s.length() - 1; i = i+2) {
           for(int j = 0; j < s.charAt(i+1) - '0'; j++) {
               System.out.print(s.charAt(i));
           }
       }
    }
}





