import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        long a=sc.nextLong(),count=0;
        while(a!=0){
            count++;
            a=a/10;
        }
        System.out.println(count);
    }
}
