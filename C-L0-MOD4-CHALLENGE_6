import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        long a=sc.nextLong(),sum=0,rem;
        while(a!=0){
            rem=a%10;
            sum=sum*10+rem;
            a=a/10;
        }
        System.out.println(sum);
    }
}
